@model Voluntinder.Models.ProfileViewModel 

<div class="profile" class="col-md-12">
    <h2>Model.Name</h2>
    <ul>
        @foreach (var skill in Model.Skills)
        {
            <li>@skill.Name</li>
        }
    </ul>
    <p>Model.ImageUrl</p>
    <p>Model.Description</p>
    <p>Model.UserId</p>
</div>